file "$(MEASCOMP)/measCompApp/Db/measCompLongIn.template"
{
pattern
{    P,       R,   MASK,   PORT,    ADDR,  TIMEOUT}
{   1608G:,  Li,   0xFF,  1608G_1,     0,        1}
}

