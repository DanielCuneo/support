# Direction bits on binary I/O
#  VAL 0=input, 1=output
file "$(MEASCOMP)/measCompApp/Db/measCompBinaryDir.template"
{
pattern
{    P,       R,   MASK,  VAL,  PORT,    ADDR,  TIMEOUT}
{   1608G:,  Bd1,  0x01    0,  1608G_1,     0,        1}
{   1608G:,  Bd2,  0x02    0,  1608G_1,     0,        1}
{   1608G:,  Bd3,  0x04    0,  1608G_1,     0,        1}
{   1608G:,  Bd4,  0x08    0,  1608G_1,     0,        1}
{   1608G:,  Bd5,  0x10    1,  1608G_1,     0,        1}
{   1608G:,  Bd6,  0x20    1,  1608G_1,     0,        1}
{   1608G:,  Bd7,  0x40    1,  1608G_1,     0,        1}
{   1608G:,  Bd8,  0x80    1,  1608G_1,     0,        1}
}

